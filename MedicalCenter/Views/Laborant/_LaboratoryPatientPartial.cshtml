@model DashboardLaboratoryPatientViewModel

<tr>
    <td scope="col">@Model.FirstName @Model.LastName</td>
    <td scope="col">@Model.Egn</td>
    <td scope="col" class="text-center">@Model.PhoneNumber</td>
    <td scope="col" class="text-center">@Model.JoinOnDate</td>
    <td scope="col" class="text-center">
        <form asp-controller="Laborant" asp-action="UploadResult" method="get">
            <input name="patientId" type="hidden" value="@Model.Id">
            <button type="submit" class="btn btn-warning">Качи</button>
        </form>
    </td>
</tr>
