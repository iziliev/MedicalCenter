<td scope="row" class="text-center">
    @for (var i = 1; i <= 5; i++)
    {
        if (i <= @Model.Rating)
        {
            <img src="~/images/star-fill.png" />
        }
        else
        {
            <img src="~/images/star-not.png" />
        }
    }
</td>
<td scope="row" class="text-center">@Model.CreatedOn</td>
