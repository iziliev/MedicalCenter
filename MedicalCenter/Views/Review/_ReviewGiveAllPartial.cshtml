@model AllGiveReviewViewModel

<tr>
    <td scope="row">д-р @Model.DoctorFullName</td>
    <td scope="row">@Model.SpecialityName</td>
    <td scope="row" style="text-align:justify;font-size:14px;">
        <div>
            <em>@Model.Content</em>
        </div>
    </td>
    <td scope="row" class="text-center">
        @for (var i = 1; i <= 5; i++)
        {
            if (i <= @Model.Rating)
            {
                <img src="~/images/star-fill.png"/>
            }
            else
            {
                <img src="~/images/star-not.png"/>
            }
        }
    </td>
    <td scope="row" class="text-center">@Model.CreatedOn</td>
</tr>
